{
  "expo": {
    "scheme": "ppdb",
    "name": "ppdb",
    "slug": "ppdb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/LexendDeca-Black.ttf",
            "./assets/fonts/LexendDeca-Bold.ttf",
            "./assets/fonts/LexendDeca-ExtraBold.ttf",
            "./assets/fonts/LexendDeca-ExtraLight.ttf",
            "./assets/fonts/LexendDeca-Light.ttf",
            "./assets/fonts/LexendDeca-Medium.ttf",
            "./assets/fonts/LexendDeca-Regular.ttf",
            "./assets/fonts/LexendDeca-SemiBold.ttf",
            "./assets/fonts/LexendDeca-Thin.ttf"
          ],
          "cache": true
        }
      ]
    ]
  }
}
